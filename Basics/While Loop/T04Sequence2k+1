using System;

namespace T04Sequence2k_1
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            int number = 1;

            while (number <= n)
            {
                Console.WriteLine(number);
                number = 2 * number + 1;
            }
            

        }
    }
}
