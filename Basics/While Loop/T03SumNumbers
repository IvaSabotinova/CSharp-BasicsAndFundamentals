using System;

namespace T03SumNumbers
{
    class Program
    {
        static void Main(string[] args)
        {
            int input = int.Parse(Console.ReadLine());
            int sum = 0;
            while (sum < input)
            {
                int currNumber = int.Parse(Console.ReadLine());
                sum += currNumber;
                            
            }
            Console.WriteLine(sum);
        }
    }
}
