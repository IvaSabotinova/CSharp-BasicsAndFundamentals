using System;

namespace T07SumNumbers
{
    class Program
    {
        static void Main(string[] args)
        {
            int number = int.Parse(Console.ReadLine());
            int sum = 0;

            for (int i = 1; i <= number; i++)
            {
                int figure = int.Parse(Console.ReadLine());

                sum += figure;
            }
            Console.WriteLine(sum);
            
                
            
        }

    }
}
