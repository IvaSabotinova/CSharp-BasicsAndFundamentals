using System;

namespace T03AnimalType
{
    class Program
    {
        static void Main(string[] args)
        {
            string typeFood = Console.ReadLine();
            switch (typeFood)
            { 
                case "banana":
            case "apple":
            case "kiwi":
            case "cherry":
            case "lemon":
            case "grapes":
                    Console.WriteLine("fruit"); break;
                case "tomato":
                case "cucumber":
                case "pepper":
                case "carrot":
                    Console.WriteLine("vegetable"); break;
                default:
                    Console.WriteLine("unknown");
                    break;
            }
        }
    }
}
